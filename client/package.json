    "react-intl": "^2.4.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.0",
    "recompose": "^0.26.0",
    "redbox-react": "^1.5.0",
    "redux": "^3.7.2",
